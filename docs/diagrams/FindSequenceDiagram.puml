@startuml
!include style.puml

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":SmartNusParser" as SmartNusParser LOGIC_COLOR
participant ":FindCommandParser" as FindCommandParser LOGIC_COLOR 
participant "f:FindCommand" as FindCommand LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant ":ModelManager" as ModelManager MODEL_COLOR
end box

[-> LogicManager : execute("find class t/CS2103T")
activate LogicManager

LogicManager -> SmartNusParser : parseCommand("find class t/CS2103T")
activate SmartNusParser

create FindCommandParser
SmartNusParser -> FindCommandParser
activate FindCommandParser
FindCommandParser -> SmartNusParser
deactivate FindCommandParser

SmartNusParser -> FindCommandParser : parse("class t/CS2103T")

activate FindCommandParser

'FindCommandParser -> FindCommandParser : setNamePredicate(input, predicates)
'activate FindCommandParser
'FindCommandParser --> FindCommandParser
'deactivate FindCommandParser
'
'FindCommandParser -> FindCommandParser : setTagsPredicate(input, predicates)
'activate FindCommandParser
'FindCommandParser --> FindCommandParser
'deactivate FindCommandParser
'
'FindCommandParser -> FindCommandParser : setImportancePredicate(input, predicates)
'activate FindCommandParser
'FindCommandParser --> FindCommandParser
'deactivate FindCommandParser

create FindCommand
FindCommandParser -> FindCommand
activate FindCommand

FindCommand --> FindCommandParser : f
deactivate FindCommand

FindCommandParser --> SmartNusParser : f
deactivate FindCommandParser

destroy FindCommandParser

SmartNusParser --> LogicManager : f
deactivate SmartNusParser

LogicManager -> FindCommand : execute(model)
activate FindCommand

FindCommand -> ModelManager : execute(model)
activate ModelManager

ModelManager -> ModelManager : updateFilteredQuestionList(predicate)
activate ModelManager
ModelManager --> ModelManager
deactivate ModelManager
ModelManager --> FindCommand
deactivate ModelManager


FindCommand --> LogicManager : result
deactivate FindCommand
FindCommand -[hidden]-> LogicManager : result
destroy FindCommand

[<--LogicManager
deactivate LogicManager
@enduml
